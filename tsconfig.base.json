    "compilerOptions": {
        "target": "ES5",
        "module": "ESNext",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": false,
        "moduleResolution": "Node",
        "resolveJsonModule": true
    }
}